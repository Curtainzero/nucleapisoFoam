FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      moment.3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    input1
    {
        type            fixedValue;
        value           uniform 0;
    }
    input2
    {
        type            fixedValue;
        value           uniform 0;
    }
    output
    {
        type            zeroGradient;
    }
    boundary
    {
        type            zeroGradient;
    }
}

